<odoo>
    <data>
		<template id='list_teachers'>
			<title>Page Title Here Academy</title>
			<t t-foreach='teachers' t-as='teacher'>
				<p> <t t-esc='teacher'/> </p>
			</t>
            <h1> Array Items from DB Below </h1>
			<t t-foreach='all_teachers' t-as='object'>
				<p> <t t-esc ='object.id'/> <t t-esc='object.name'/> </p>
			</t>
		</template>

      <template id="product_item_hide_no_price" inherit_id="website_sale.products_item">
         <xpath expr="//div[hasclass('product_price')]/b" position="attributes">
            <attribute name="t-if">product.price &gt; 0</attribute>
         </xpath>
      </template>

		<record id='website_sale.products_categories' model='ir.ui.view'>
			<field name='active' eval='True'/>
		</record>


